@{
    ViewBag.Title = "Administration";
}

<div class="container">
    <div class="row">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">
                    <span class="glyphicon glyphicon-bookmark"></span> Dashboard administratif
                </h3>
            </div>
            <div class="panel-body">
                <div class="row">
                    <a href=@Url.Action("ProfilManager", "Admin") class="btn btn-success btn-lg" role="button"><span class="glyphicon glyphicon-user"></span> <br />Utilisateurs</a>
                    <a href=@Url.Action("CompetitionManager", "Admin") class="btn btn-danger btn-lg" role="button"><span class="glyphicon glyphicon-list-alt"></span> <br />Compétitions</a>
                    <a href=@Url.Action("CategorieManager", "Admin") class="btn btn-warning btn-lg" role="button"><span class="glyphicon glyphicon-list"></span> <br />Catégories</a>
                    <a href=@Url.Action("ClubManager", "Admin") class="btn btn-info btn-lg" role="button"><span class="glyphicon glyphicon-folder-open"></span> <br />Clubs</a>
                    <a href=@Url.Action("NageursManager", "Admin") class="btn btn-primary btn-lg" role="button"><span class="glyphicon glyphicon-tag"></span> <br />Nageurs</a>
                </div>
            </div>
        </div>
    </div>
</div>
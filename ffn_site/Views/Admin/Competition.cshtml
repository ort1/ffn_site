@using ffn_site.Models;
@{
    ViewBag.Title = "Gestion de compétition";
}
    
@section scripts {
    <script>
        $( function() {
            $(".accordion").accordion({
                active: false,
                collapsible: true
            });
        } );
    </script>
}

<div class="container">
    <div class="row">
        <h2>Types catégories</h2>
        <div class="accordion col-lg-10">
            @Html.Partial("Categories/_CategoriesCompetition")

            @Html.Partial("Categories/_CategoriesTour")

            @Html.Partial("Categories/_CategoriesEpreuve")

            @Html.Partial("Categories/_CategoriesBallet")
        </div>
    </div>

    <div class="row">
        <h2>Gestion des compétitions</h2>
        <a href="#">Ajout</a>
        <ul id="listeCompetitions">
            <li>comp 1 + infos</li>
            <li>comp 2 + infos</li>
            <li>comp 3 + infos</li>
            <li>comp 4 + infos</li>
        </ul>
    </div>
</div>